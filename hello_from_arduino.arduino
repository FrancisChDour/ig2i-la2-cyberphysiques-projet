void setup() {
  Serial1.begin(115200);
  Serial2.begin(115200);
  Serial3.begin(115200);
  Serial.begin(115200);
}
void loop() {
  Serial.println("Hello from Serial");
  Serial1.println("Hello from Serial1");
  Serial2.println("Hello from Serial2");
  Serial3.println("Hello from Serial3");
  delay(1000);
}
